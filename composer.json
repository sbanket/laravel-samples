{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "doctrine/collections": "^1.5",
    "doctrine/dbal": "~2.3",
    "fideloper/proxy": "~3.3",
    "intervention/image": "^2.4",
    "laracasts/flash": "^3.0",
    "laravel/framework": "5.5.*",
    "laravel/tinker": "~1.0",
    "lavary/laravel-menu": "^1.7",
    "league/flysystem-aws-s3-v3": "^1.0",
    "mark-gerarts/auto-mapper-plus": "^1.0",
    "rcrowe/twigbridge": "^0.9.5",
    "santigarcor/laratrust": "^5.0.0",
    "barryvdh/laravel-dompdf": "^0.8.1"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.4",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.0",
    "codeception/codeception": "2.3.*"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "Axmit\\Image\\": "module/Axmit/Image/src",
      "Axmit\\Notification\\": "module/Axmit/Notification/src",
      "Axmit\\FeatureToggle\\": "module/Axmit/FeatureToggle/src",
      "Project\\Achievement\\": "module/Project/Achievement/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
